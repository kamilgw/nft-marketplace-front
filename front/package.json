{
  "name": "front",
  "version": "0.1.0",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "heroku-postbuild": "yarn run build",
    "lint": "eslint pages src --cache",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "analyze": "ANALYZE=true yarn build",
    "create-local-certs": "mkdir -p certs && mkcert  -cert-file certs/cert.pem -key-file certs/key.pem localhost 127.0.0.1 ::1",
    "codegen": "graphql-codegen --config ./codegen.yml"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@graphql-codegen/typescript-operations": "^2.1.3",
    "@graphql-codegen/typescript-react-apollo": "^3.1.3",
    "@lay2/pw-core": "^0.3.22",
    "@sentry/nextjs": "^6.8.0",
    "@types/date-fns": "^2.6.0",
    "antd": "^4.15.6",
    "aws-sdk": "^2.987.0",
    "axios": "^0.21.1",
    "babel-plugin-import": "^1.13.3",
    "child_process": "^1.0.2",
    "fs": "^0.0.1-security",
    "graphql": "^15.5.2",
    "jwt-decode": "^3.1.2",
    "next": "10.2.0",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.2",
    "react-use": "^17.2.4",
    "recharts": "^2.0.9",
    "tls": "^0.0.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@graphql-codegen/cli": "^2.1.1",
    "@next/bundle-analyzer": "^9.5.0",
    "@types/jest": "^26.0.23",
    "@types/node": "12.0.12",
    "@types/qrcode.react": "^1.0.1",
    "@types/react": "^17.0.6",
    "@types/react-cookies": "^0.1.0",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-csstyper": "^0.3.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.5",
    "jest": "^26.6.3",
    "next-compose-plugins": "^2.2.1",
    "next-plugin-antd-less": "^1.2.1",
    "prettier": "^2.3.0",
    "sass": "^1.39.2",
    "typescript": "^4.2.4"
  }
